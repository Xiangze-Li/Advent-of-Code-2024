  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}",
      "args": ["${input:Day}", "${input:Part}"],
    }
  ],
  "inputs": [
    {
      "id": "Day",
      "type": "promptString",
      "description": "Day",
    },
    {
      "id": "Part",
      "type": "pickString",
      "description": "Part",
      "options": ["1", "2", "3"],
      "default": "3"
    }
  ]
}
